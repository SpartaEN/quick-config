Description='A basic static ethernet connection'
Interface=ens3
Connection=ethernet
IP=static
TimeoutDAD=10
Address=('**IPv4 with mask**')
Gateway='**IPv4 of G/W**'
IP6=stateless
Address6=('**IPv6 with CDIR**')
DNS=('108.61.10.10' '2001:19f0:300:1704::6' '8.8.8.8' '2001:4860:4860::8888')
ExecUpPost='/usr/sbin/ip route add 169.254.0.0/16 dev ens3'
